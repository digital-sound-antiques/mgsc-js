  "bugs": {
    "url": "https://github.com/digital-sound-antiques/mgsc-js/issues"
  },
  "homepage": "https://github.com/digital-sound-antiques/mgsc-js",
  "engines": {
    "node": ">=14.18.2"
  }
}
