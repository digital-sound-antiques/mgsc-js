{
  "name": "mgsc-js",
  "version": "1.3.0",
  "description": "MML to MGS compiler for MGSDRV",
  "bin": "bin/cmd.js",
  "files": [
    "bin",
    "src",
    "lib",
    "README.md",
    "package.json"
  ],
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "dependencies": {
    "encoding-japanese": "^2.0.0",
    "minimist": "^1.2.8",
    "rimraf": "^4.4.1"
  },
  "devDependencies": {
    "cpy-cli": "^4.2.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib build",
    "build": "npm run cmake",
    "cmake": "mkdirp build && cd build && emcmake cmake .. && make && cpy libmgsc.js ../lib/",
    "jsdoc": "jsdoc -c jsdoc.config.json --readme README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-sound-antiques/mgsc-js.git"
  },
  "keywords": [],
  "author": "Mitsutaka Okazaki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/digital-sound-antiques/mgsc-js/issues"
  },
  "homepage": "https://github.com/digital-sound-antiques/mgsc-js"
}
